function load_libary()
    
    local library = load_lib()

local Main = library.initiate("Aztech Hub", {
	default = UDim2.new(0, 520, 0, 520),
	resize = true,
	min = UDim2.new(0, 300, 0, 300),
	max = UDim2.new(0, 520, 0, 520)
})

if PROTOSMASHER_LOADED then
	exploit_type = "ProtoSmasher"
elseif is_sirhurt_closure then
	exploit_type = "Sirhurt"
elseif SENTINEL_LOADED then
	exploit_type = "Sentinel"
elseif syn then
	exploit_type = "Synapse X"
elseif KRNL_LOADED then
    exploit_type = "KRNL"
elseif FLUXUS_LOADED then
    exploit_type = "Fluxus"
elseif getexecutorname then 
	exploit_type = "Script-Ware"
else
    exploit_type = "Not Found"
end

if game.PlaceId == 13822889 then
    detected = "Lumber Tyccon 2"
end
if game.PlaceId == 3956818381 then
    detected = "Ninja Legends"
end

local user = game.Players.LocalPlayer.Name

local InfoTab = Main.category("Information")

local InfoSection = InfoTab.section("User Info")

InfoSection.label("Game Detected - "..tostring(detected))

InfoSection.label("Executer used - "..exploit_type)

InfoSection.label("Roblox Username - "..user)

local CreditsSection = InfoTab.section("Credits")

CreditsSection.label("Main Scripter - Selync#6732")

CreditsSection.label("Key System - Selync#6732")

CreditsSection.label("UI Lib - xTheAlex14#3200")

local CreditsThemeSection = InfoTab.section("Theme Credits")

CreditsThemeSection.label("Normal Theme - xTheAlex14#3200")

CreditsThemeSection.label("Light Theme - Selync#6732")
    
end
